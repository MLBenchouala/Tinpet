<%= simple_form_for @user do |f| %>
  <%= f.input :name %>
  <%= f.input :age %>
  <%= f.input :photos, as: :file, input_html: { multiple: true } %>
  <%= f.input :gender %>
  <%= f.input :orientation %>
  <%= f.input :description %>
  <%= f.input :walk %>
  <%= f.input :more %>
  <%= f.submit %>
  <% @user.photos.each do |photo| %>
  <%= cl_image_tag photo.key, height: 300, width: 400, crop: :fill %>
  <% end %>
<% end %>
<%= link_to 'list all pets', pets_path %>
